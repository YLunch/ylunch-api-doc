        "responses" : {
          "201" : {
            "description" : "user created",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/UserReadDto"
                }
              }
            }
          },
          "400" : {
            "description" : "invalid input, object invalid"
          },
          "409" : {
            "description" : "User already exists"
          }
        }
      }
    },
    "/customers" : {
      "post" : {
        "tags" : [ "users" ],
        "summary" : "Customer registration",
        "operationId" : "addCustomer",
