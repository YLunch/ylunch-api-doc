          "id" : {
            "type" : "string",
            "example" : "a00000e0-aaa0-000a-0000-00e00aa00e00"
          },
          "email" : {
            "type" : "string",
            "format" : "email",
            "example" : "user@domain.com"
          },
          "firstname" : {
            "type" : "string",
            "example" : "Jean"
          },
          "lastname" : {
            "type" : "string",
            "example" : "Dupont"
          },
          "phoneNumber" : {
            "type" : "string",
            "format" : "phone",
            "example" : "0612345678"
